{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/Shop.Catalog.UnitTest/bin/Debug/net6.0/Shop.Catalog.UnitTest.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Shop.Catalog.UnitTest",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Docker .NET Core Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "netCore": {
        "appProject": "${workspaceFolder}/Shop.Catalog.UnitTest/Shop.Catalog.UnitTest.csproj"
      }
    }
  ]
}
